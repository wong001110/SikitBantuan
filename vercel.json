{
    "buildCommand": "cd client && yarn install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "https://sikit-bantuan-api.vercel/(.*)",
            "destination": "https://sikit-bantuan-api.vercel.app/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}